{
  "extends": "airbnb-base",
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true
  },
  "rules": {
    "no-param-reassign": ["error", { "props": false }],
    "template-curly-spacing": ["error", "always"],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": ["webpack.*.js"] }]
  }
}
